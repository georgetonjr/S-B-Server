generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id
  name        String
  email       String   @unique
  password    String
  cpf         String   @unique
  phone       String   @unique
  createdDate DateTime @default(now())
}
